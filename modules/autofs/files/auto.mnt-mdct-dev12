# home directory on corporate file server
files-2k1-d13677        -fstype=cifs,uid=d13677,gid=d13677,credentials=/mnt-local/storage/j/.credentials/d13677.cifs    ://files-2k1/d13677\$

# shared engineering directory
files-2k1-eng           -fstype=cifs,credentials=/mnt-local/storage/j/.credentials/d13677.cifs                          ://files-2k1/Eng

# generic shared directory
files-2k1-sharedata     -fstype=cifs,credentials=/mnt-local/storage/j/.credentials/d13677.cifs                          ://files-2k1/ShareData



# general storage not belonging to any particiluar Fedora installation
storage                 -fstype=ext4,rw         :/dev/data/storage

# Fedora 11 root file system
f11                     -fstype=ext3,ro         :/dev/fedora/f11root
