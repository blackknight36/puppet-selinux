Hostname "<%= @fqdn %>"

FQDNLookup false

LoadPlugin syslog
LoadPlugin cpu
LoadPlugin memory
LoadPlugin disk
LoadPlugin load
LoadPlugin interface
LoadPlugin network

<Plugin "network">
    <Server "<%= @collectd_server %>">
        SecurityLevel "Encrypt"
        Username "<%= @collectd_user %>"
        Password "<%= @collectd_pass %>"
    </Server>
</Plugin>

