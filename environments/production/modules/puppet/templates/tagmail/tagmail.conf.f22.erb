# https://forge.puppetlabs.com/puppetlabs/tagmail#usage

[transport]
reportfrom = report@puppet.dartcontainer.com
smtpserver = smtp.dartcontainer.com
smtpport = 25
smtphelo = <%= @fqdn %>

[tagmap]
all, !mdct_dev27, !mdct_dev27_kstest, !mdct_dev27_graphite, !mdct_dev27_puppetdb: <%= @puppet_admins %>
tc_util: <%= @tcutil_admins %>
